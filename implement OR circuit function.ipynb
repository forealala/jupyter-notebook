    "def OR(x1, x2):\n",
    "    a1, a2, b = 0.3, 0.3, 0.4\n",
    "    delta = 0.5\n",
    "    y = a1*x1 + a2*x2 +b\n",
    "    \n",
    "    if y < delta:\n",
    "        return 0\n",
    "    else:\n",
    "        return 1"
